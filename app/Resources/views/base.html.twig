<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}InstaWall{% endblock %}</title>
        
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        {% block stylesheets %}
	        {% stylesheets  
				'@DC3InstawallBundle/Resources/public/css/bootstrap.css'
				 filter='cssrewrite'
			%}
	            <link href="{{ asset_url }}" rel="stylesheet" />
	        {% endstylesheets %}
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
		<!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
		<!--[if lt IE 9]>
		<script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
		<script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
		<![endif]-->
		{% block maps %}{% endblock %}
		{% javascripts '@DC3InstawallBundle/Resources/public/js/modernizr.min.js'
		'@DC3InstawallBundle/Resources/public/js/classie.js'
        '@DC3InstawallBundle/Resources/public/js/photostack.js'
        '@DC3InstawallBundle/Resources/public/js/maps.js'
		%}
		    <script src="{{ asset_url }}"></script>
		{% endjavascripts %}	
    </head>
    <body style screen_capture_injected="true">
	   
		    {% block body %}{% endblock %}
     
        <script src="https://code.jquery.com/jquery.js"></script>
        {% javascripts 
        	'@DC3InstawallBundle/Resources/public/js/main.js'
        	'@DC3InstawallBundle/Resources/public/js/bootstrap.js'
			'@DC3InstawallBundle/Resources/public/js/script.js'
		%}
		    <script src="{{ asset_url }}"></script>
		{% endjavascripts %}
		{% block photostack %}{% endblock %}
    </body>
</html>
